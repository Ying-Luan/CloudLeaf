{
  "name": "cloud-leaf",
  "displayName": "__MSG_extension_displayName__",
  "version": "0.0.2",
  "description": "Browser Bookmark Cloud Sync Extension.",
  "author": "Ying Luan & KamikazEr101",
  "scripts": {
    "dev": "plasmo dev",
    "dev:edge": "plasmo dev --target=edge-mv3",
    "dev:firefox": "plasmo dev --target=firefox-mv2",
    "build": "plasmo build",
    "build:edge": "plasmo build --target=edge-mv3",
    "build:firefox": "plasmo build --target=firefox-mv2",
    "package": "plasmo package",
    "package:edge": "plasmo package --target=edge-mv3",
    "package:firefox": "plasmo package --target=firefox-mv2",
    "postinstall": "node scripts/patch.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "immer": "^11.1.3",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "^2.0.7",
    "sweetalert2": "^11.26.20",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.2.1",
    "@types/chrome": "0.0.258",
    "@types/firefox-webext-browser": "^143.0.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "eslint": "^9.39.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tsdoc": "^0.5.2",
    "globals": "^17.3.0",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.56.1"
  },
  "manifest": {
    "default_locale": "en",
    "permissions": [
      "bookmarks",
      "sidePanel",
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ]
  }
}
